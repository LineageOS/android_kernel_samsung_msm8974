&soc {
        i2c@15{
                cell-index = <15>;
                compatible = "i2c-gpio";
                gpios = <&msmgpio 8 0  /*SDA*/
                        &msmgpio 9 0  /*SCL*/
                        >;
                i2c-gpio,delay-us = <2>;
                #address-cells = <1>;
                #size-cells = <0>;
                rt8973@14{
                        compatible = "rt8973,i2c";
                        reg = <0x14>;
                        interrupt-parent = <&msmgpio>;
                        interupts= <83 0>;
                        rt8973,irq-gpio = <&msmgpio 83 0x00>;
                };
        };
};
