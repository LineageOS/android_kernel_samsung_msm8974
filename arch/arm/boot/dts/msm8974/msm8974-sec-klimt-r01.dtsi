	qcom,hdmi_tx@fd922100 {
		status = "ok";
	};

    i2c@13 {
        cell-index = <13>;
        compatible = "i2c-gpio";
        gpios = <&msmgpio 83 0 /* sda */
                 &msmgpio 84 0 /* scl */
                >;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		max77888@66 {
		    compatible = "max77888,i2c";
		    reg = <0x66>;
		    interrupt-parent = <&pm8941_gpios>;
		    interrupts = <23 0>,
		    		 <27 0>;
		    max77888,irq-gpio = <&pm8941_gpios 23 0x00>;
		    max77888,irq-reset-gpio = <&pm8941_gpios 27 0x00>;
		    max77888,irq-base = <357>;
		    max77888,wakeup = <1>;
		    max77888,haptic = <&haptic_pdata>;
		};
    };

	i2c@22 {
		cell-index = <22>;
		compatible = "i2c-gpio";
		gpios = <&msmgpio 109 0 /* sda */
		&msmgpio 108 0 /* scl */
		>;

		barcode_emul_ice4@19 {
			compatible = "barcode_hlte,barcode_emul_hlte";
			reg = <0x19>;
			interrupt-parent = <&msmgpio>;
			interrupts = <64 0>;
			vdd-supply = <&pm8941_l19>;
			barcode,cresetb = <&msmgpio 57 0x01>;
			barcode,reset_n = <&msmgpio 132 0x00>;
			barcode,scl-gpio = <&msmgpio 108 0x00>;
			barcode,sda-gpio = <&msmgpio 109 0x00>;
			barcode,irq-gpio = <&msmgpio 64 0x00>;
			barcode,i2c-pull-up = <1>;
		};
	};

	i2c@23 {
		cell-index = <23>;
		compatible = "i2c-gpio";
		gpios = <&msmgpio 95 0 /* sda */
		&msmgpio 96 0 /* scl */
		>;
		sx9500-i2c@28 {
			compatible = "sx9500-i2c";
			reg = <0x28>;
			sx9500-i2c,nirq-gpio = <&pm8941_mpps 2 0x00>;
			sx9500-i2c,scl-gpio = <&msmgpio 96 0x00>;
			sx9500-i2c,sda-gpio = <&msmgpio 95 0x00>;
			sx9500-i2c,i2c-pull-up = <1>;
		};
	};

	i2c@24 {
		cell-index = <24>;
		compatible = "i2c-gpio";
		gpios = <&msmgpio 10 0 /* sda */
				&msmgpio 11 0 /* scl */
		>;


		cypress_touchkey@20 {
			compatible = "cypress,cypress-tkey";
			reg = <0x20>;
			interrupt-parent = <&msmgpio>;
			interrupts = <144 0>;
			vdd_led-supply = <&pm8941_l19>;
			vcc_en-supply = <&pm8941_l10>;
			vdd_led-gpio = <&pm8941_gpios 12 0x00>; /* TCON_EN */
			cypress,scl-gpio = <&msmgpio 11 0x00>;
			cypress,sda-gpio = <&msmgpio 10 0x00>;
			cypress,irq-gpio = <&msmgpio 144 0x1>;
			cypress,tkey-keycodes = <254 158>;
			cypress,i2c-pull-up = <1>;
		};

